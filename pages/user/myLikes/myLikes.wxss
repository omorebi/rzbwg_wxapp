/* pages/user/myLikes/myLikes.wxss */
page {
    background-color: #F4F4F4;
}
.container_box {
    width: 100%;
    background-color: #fff;
    padding-top: 20rpx;
}
.like_items {
    width: 700rpx;
    height: 248rpx;
    padding: 24rpx 0;
    box-sizing: border-box;
    margin: 0 auto;
    border-bottom: 1rpx solid #E5E5E5;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.item_left {
    width: 260rpx;
    height: 200rpx;
    border-radius: 12rpx;
    border: 4rpx solid #B49B96;
    overflow: hidden;
}
.item_left image {
    width: 260rpx;
    height: 200rpx;
}
.item_right {
    width: 440rpx;
    height: 248rpx;
    padding: 24rpx 60rpx 24rpx 30rpx ;
    box-sizing: border-box;
    position: relative;
}
.name {
    font-size: 30rpx;
    font-weight: 500;
    color: #333333;
}
.date {
    font-size: 26rpx;
    color: #999999;
    margin-top: 30rpx;
}

/* 删除按钮 */
.delete_btn {
    width: 33rpx;
    height: 30rpx;
    position: absolute;
    right: 0;
    top: 24rpx;
}